{
  "name": "plugins-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "pnpm turbo dev --filter=test-studio",
    "format": "oxfmt . --no-error-on-unmatched-pattern",
    "lint": "turbo run oxlint lint",
    "lint:ci": "turbo run lint -- -f gha --max-warnings 0",
    "oxlint": "oxlint --type-aware --deny-warnings",
    "release": "changeset publish",
    "typecheck": "turbo typecheck --continue"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@typescript/native-preview": "catalog:",
    "eslint-formatter-gha": "^1.6.0",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "oxlint-tsgolint": "^0.8.3",
    "setup-npm-trusted-publish": "^1.0.3",
    "turbo": "^2.6.3",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.24.0"
}
